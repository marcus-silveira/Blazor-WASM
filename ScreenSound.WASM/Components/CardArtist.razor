<MudCard>
    <MudCardMedia Image="images/cardArtista.png"></MudCardMedia>
    <MudCardHeader>
        <CardHeaderContent>
            <MudText Typo="Typo.h6">@Artist!.Name</MudText>
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardContent>
        <MudText Typo="Typo.body2">@Artist!.Bio</MudText>
    </MudCardContent>
</MudCard>

@code {
    [Parameter]
    public ArtistResponse? Artist { get; set; }
}